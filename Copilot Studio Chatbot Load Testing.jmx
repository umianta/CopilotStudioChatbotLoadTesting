            <stringProp name="script">import com.jayway.jsonpath.JsonPath ;
String response =prev.getResponseDataAsString();
def conversationId_SearchStr=&apos;$.conversationId&apos;;
def token_SearchStr=&apos;$.token&apos;;
def conversationId=JsonPath.read(response,conversationId_SearchStr);
def token = JsonPath.read(response,token_SearchStr);

